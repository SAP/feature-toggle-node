{
  "name": "@sap-devx/feature-toggle-node",
  "version": "1.0.5",
  "description": "",
  "main": "lib/api.js",
  "types": "lib/api.js",
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": ">=10.19"
  },
  "scripts": {
    "ci": "npm-run-all compile lint coverage",
    "ci_t": "npm-run-all compile lint",
    "compile": "npm run clean && tsc -p .",
    "test": "mocha",
    "lint": "eslint . --max-warnings 0 --ext .ts",
    "coverage": "nyc mocha",
    "clean": "rimraf ./lib",
    "watch": "tsc -p . --watch",
    "preversion": "npm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/SAP/feature-toggle-node.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "@types/rimraf": "3.0.0",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "chai": "^4.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "mocha": "^8.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "^9.0.2",
    "source-map-support": "0.5.19",
    "ts-node": "8.10.2",
    "typescript": "3.9.7" 
  },
  "mocha": {
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "spec": "./test/**/*spec.ts"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/example/example.ts",
      "src/example/example_simple.ts",
      "src/unleash_client_wrapper.ts"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "check-coverage": true,
    "all": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "dependencies": {
    "await-to-js": "^2.1.1",
    "parse-duration": "^0.4.4",
    "unleash-client": "^3.3.5"
  }
}
